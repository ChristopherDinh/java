package demo;

public class demo {

	public static void main(String[] args) {
		System.out.println("welcome to the method definiton app");

				System.out.println("Hello");
				
				String[][] myArray = new String[10][5];
				
				for (int row = 0; row < 5; row++) {
					for (int col=0; col < 10; col++) {
						myArray[row][col] = row +" "+ col;
						System.out.println(myArray[row][col]);
					}
				}	for (int row = 0; row < myArray.length; row++) {
			String rowStr = "";
			for (int col = 0; col < myArray[col].length; col++) {
				rowStr+=myArray[row][col]+";";
			}
			System.out.println(rowStr);
		}
	}
	
	

}
